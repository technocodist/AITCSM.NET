<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        x:Class="AITCSM.NET.UI.MainWindow"
        Width="800" Height="600"
        Title="AITCSM.NET Simulation UI">
    <StackPanel Margin="20">
        <TextBlock Text="Simulation List" FontWeight="Bold" FontSize="20" Margin="0,0,0,10"/>
        <ListBox Name="SimulationListBox" Height="200"/>
        <Button Name="StartButton" Content="Start Simulation" Margin="0,10,0,0"/>
        <TextBlock Name="ResultTextBlock" Margin="0,10,0,0"/>
    </StackPanel>
</Window>
